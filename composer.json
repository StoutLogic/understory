{
    "name": "stoutlogic/understory",
    "description": "A WordPress theme layer, built on Timber and Twig",
    "type": "wordpress-muplugin",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Steve Pfisterer",
            "email": "steve@stoutlogic.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "composer/installers": "~1.0",
        "timber/timber": "~1.3.0",
        "twig/twig": "^1.0|^2.0",
        "wemakecustom/wp-mu-loader": "*",
        "doctrine/inflector": "^1.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "mockery/mockery": "^0.9.5",
        "php-mock/php-mock-mockery": "^1.1",
        "brain/monkey": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Understory\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Understory\\Tests\\": "tests/"
        }
    }
}
